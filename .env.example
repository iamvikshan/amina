# Discord Bot Configuration
CLIENT_ID=your_discord_client_id
CLIENT_SECRET=your_discord_client_secret
BOT_TOKEN=your_bot_token

# Application URLs
BASE_URL=https://your-domain.com
DOCS_URL=https://docs.your-domain.com
SUPPORT_SERVER=https://discord.gg/your-invite
BOTS_URL=https://your-bots-url.com

# Database
MONGO_CONNECTION=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# API Keys (Optional - only if using these features)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
OPENAI=your_openai_key
STRANGE_API_KEY=your_strange_api_key

# Session & Security
SESSION_PASSWORD=your_secure_random_session_password

# Developer Settings
DEV_ID=your_discord_user_id
TEST_GUILD_ID=your_test_guild_id

# Webhooks & Logging (Optional)
LOGS_WEBHOOK=your_discord_webhook_url
HONEYBADGER_API_KEY=your_honeybadger_key
HONEYBADGER_ENV=production

# Lavalink Music Nodes (Optional - if using music features)
LAVALINK_HOST_1=your_lavalink_host
LAVALINK_PORT_1=your_lavalink_port
LAVALINK_PASSWORD_1=your_lavalink_password
LAVALINK_SECURE_1=false
LAVALINK_ID_1=node1

# Build Settings
PORT=4321
GLOBAL=true
DASH=true
