# ============================================
# REQUIRED - Bot will not start without these
# ============================================

# Bot Token from Discord Developer Portal
BOT_TOKEN=

# MongoDB Connection String
MONGO_CONNECTION=

# ============================================
# OPTIONAL - Core Features
# ============================================

# Secure random string for dashboard webhook authentication, must match the one in your dashboard
WEBHOOK_SECRET=

# Discord webhook URL for bot logs (Shoutrrr format for Watchtower compatibility)
# Convert: https://discord.com/api/webhooks/ID/TOKEN -> discord://TOKEN@ID
LOGS_WEBHOOK=

# ============================================
# LAVALINK - Music System
# ============================================
# Add any number of lavalink nodes here
# Refer to https://github.com/freyacodes/Lavalink to host your own

# Primary Node (local Docker default)
LAVALINK_HOST_1=lavalink
LAVALINK_PORT_1=2333
LAVALINK_PASSWORD_1=incorrect
LAVALINK_ID_1=Lavalink
LAVALINK_SECURE_1=false

# Secondary Node (fallback)
LAVALINK_HOST_2=
LAVALINK_PORT_2=
LAVALINK_PASSWORD_2=
LAVALINK_ID_2=
LAVALINK_SECURE_2=false

# ============================================
# MUSIC SERVICES - For Lavalink/LavaSrc, must have local hosted lavalink, else skip.
# ============================================

# Spotify API credentials (https://developer.spotify.com/dashboard)
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=

# ============================================
# AI & FEATURES
# ============================================

# Google Gemini API Key (https://makersuite.google.com/app/apikey)
GEMINI_KEY=

# Upstash Vector Database token (https://upstash.com)
UPSTASH_VECTOR=

# ============================================
# UTILITY COMMANDS
# ============================================

# Weather command (https://weatherstack.com)
WEATHERSTACK_KEY=

# Image manipulation commands (https://strangeapi.hostz.me/docs/)
STRANGE_API_KEY=

# GitHub token for changelog command (https://github.com/settings/tokens)
# Fine-grained token with Read access to code, discussions, issues, and metadata
GH_TOKEN=

# ============================================
# MONITORING
# ============================================

# Honeybadger error tracking (https://www.honeybadger.io)
HONEYBADGER_API_KEY=