# Bot Token [Required]
BOT_TOKEN=

# Mongo Database Connection String [Required]
MONGO_CONNECTION=

# Configs [Required]
DEV_ID= # bot owner/developer id for multiple devs, use [id1, id2, id3] (square brackets included)
SUPPORT_SERVER= # support server invite link
DOCS_URL= # bot docs link
DONATE_URL= # your donations url
GITHUB_URL= # github repo url
GITHUB_SPONSORS_URL= # github sponsors url
PATREON_URL= # patreon url
BOTS_URL= # your other bots url
TEST_GUILD_ID= # test server ID

# Webhooks [Optional]
LOGS_WEBHOOK=
LOGS_WEBHOOK=
LOGS_WEBHOOK=

# Dashboard [Required for dashboard]
CLIENT_SECRET=
SESSION_PASSWORD=

# Required for Weather Command (https://weatherstack.com)
WEATHERSTACK_KEY=

# Required for image commands (https://strangeapi.hostz.me/docs/)
STRANGE_API_KEY=

# Add any number of lavalink nodes here 
# Refer to https://github.com/freyacodes/Lavalink to host your own lavalink server
# Primary (local Docker)
LAVALINK_HOST_1=lavalink
LAVALINK_PORT_1=2333
LAVALINK_PASSWORD_1=incorrect
LAVALINK_ID_1=Lavalink
LAVALINK_SECURE_1=false

# Secondary (fallback to wispbyte1)
LAVALINK_HOST_2=wispbyte1
LAVALINK_PORT_2=443
LAVALINK_PASSWORD_2=incorrect
LAVALINK_ID_2=Lavalink2
LAVALINK_SECURE_2=true

# Health check port
HEALTH_PORT=3000

# SPOTIFY [Required for Spotify Support via LavaSrc]
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=

# APPLE MUSIC [Optional for Apple Music Support via LavaSrc]
# Get token from: https://developer.apple.com/documentation/applemusicapi/getting_keys_and_creating_tokens
APPLE_MUSIC_TOKEN=

# DEEZER [Optional for native Deezer playback via LavaSrc]
# Master decryption key (advanced users only)
DEEZER_KEY=

# GENIUS [Optional for lyrics fallback via java-timed-lyrics]
# Get your Client Access Token from https://genius.com/api-clients
GENIUS_API_KEY=

# Required for OpenAI (https://beta.openai.com/)
OPENAI=


# Optional, for GitHub commands eg `bot changelog` (https://github.com/settings/tokens)
GH_TOKEN= # fine-grained token with Read access to code, discussions, issues, and metadata permissions
GH_USERNAME= # your github username
GH_REPO=amina # the name of the github repo for your bot
CHANGELOG_PATH=CHANGELOG.md # url to your bot's full changelog file EG: https://github.com/iamvikshan/amina/blob/main/src/CHANGELOG.md