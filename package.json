{
  "name": "amina",
  "version": "3.2.0",
  "description": "An open-source, multipurpose discord bot built using discord-js",
  "main": "src/index.ts",
  "author": "vikshan",
  "license": "CC-BY-4.0",
  "engines": {
    "bun": ">=1.3.8"
  },
  "workspaces": [
    "api"
  ],
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun check",
    "start": "bun src/index.ts",
    "f": "prettier --write .",
    "f:check": "prettier --check .",
    "check": "tsc --noEmit && eslint .",
    "check:watch": "tsc --noEmit --watch",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "api:dev": "bun --filter 'amina-api' dev",
    "api:check": "bun --filter 'amina-api' check",
    "api:deploy": "bun --filter 'amina-api' deploy",
    "check:all": "bun check && bun api:check"
  },
  "homepage": "https://github.com/iamvikshan/amina#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamvikshan/amina.git"
  },
  "bugs": {
    "url": "https://github.com/iamvikshan/amina/issues/new/choose"
  },
  "dependencies": {
    "@anthropic-ai/vertex-sdk": "^0.14.4",
    "@google/genai": "^1.41.0",
    "@honeybadger-io/js": "^6.12.3",
    "@octokit/rest": "^22.0.1",
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.13.5",
    "common-tags": "^1.8.2",
    "dayjs": "^1.11.19",
    "discord-gamecord": "^4.4.4",
    "discord-giveaways": "^6.0.1",
    "discord-together": "^1.4.2",
    "discord.js": "^14.25.1",
    "enhanced-ms": "^4.2.0",
    "google-auth-library": "9.15.1",
    "lavalink-client": "^2.9.2",
    "lru-cache": "^11.2.6",
    "module-alias": "^2.3.4",
    "mongoose": "^9.2.1",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "pretty-ms": "^9.3.0",
    "sourcebin_js": "^0.0.3-ignore",
    "string-progressbar": "^1.0.4",
    "table": "^6.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/bun": "^1.3.9",
    "@types/common-tags": "^1.8.4",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^10.0.1",
    "eslint-plugin-jsdoc": "^62.7.0",
    "husky": "^9.1.7",
    "prettier": "3.8.1",
    "semantic-release-gitmoji": "^1.6.9",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot",
    "open-source",
    "bot",
    "multipurpose",
    "multi-purpose-bot"
  ]
}
