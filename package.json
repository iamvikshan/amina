    "@preline/accordion": "^3.2.3",
    "@preline/collapse": "^3.2.3",
    "@preline/dropdown": "^3.2.3",
    "@preline/overlay": "^3.2.3",
    "@preline/tabs": "^3.2.3",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/alpinejs": "^3.13.11",
    "alpinejs": "^3.15.2",
    "astro": "5.16.4",
