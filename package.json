{
  "name": "discord-js-bot",
  "version": "5.6.0",
  "description": "An open-source, multipurpose discord bot built using discord-js",
  "main": "src/index.ts",
  "author": "vikshan",
  "license": "ISC",
  "module": "commonjs",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "start:node": "node dist/index.js",
    "build": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch",
    "postbuild": "bash scripts/postbuild.sh",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "f": "prettier --write .",
    "f:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "start-replit": "node src/index.js",
    "prepare": "husky"
  },
  "homepage": "https://github.com/iamvikshan/amina#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamvikshan/amina.git"
  },
  "bugs": {
    "url": "https://github.com/iamvikshan/amina/issues/new/choose"
  },
  "dependencies": {
    "@honeybadger-io/js": "^6.12.3",
    "@octokit/rest": "^22.0.1",
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.13.2",
    "btoa": "^1.2.1",
    "common-tags": "^1.8.2",
    "discord-gamecord": "^4.4.2",
    "discord-giveaways": "^6.0.1",
    "discord-together": "^1.4.2",
    "discord.js": "^14.24.2",
    "enhanced-ms": "^4.1.0",
    "fixedsize-map": "^1.1.0",
    "lavalink-client": "^2.6.5",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.19.3",
    "node-fetch": "^2.7.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "pretty-ms": "^7.0.1",
    "sourcebin_js": "^0.0.3-ignore",
    "string-progressbar": "^1.0.4",
    "table": "^6.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "bun-types": "^1.3.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-jsdoc": "^61.1.12",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "semantic-release-gitmoji": "^1.6.8",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot",
    "open-source",
    "bot",
    "multipurpose",
    "multi-purpose-bot"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@handlers": "src/handlers/",
    "@helpers": "src/helpers/",
    "@schemas": "src/database/schemas/",
    "@src": "src/",
    "@structures": "src/structures/",
    "@commands": "src/commands/"
  }
}
