    "tailwindcss": "^4.1.18",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.6",
    "@hono/vite-build": "^1.8.0",
    "@hono/vite-dev-server": "^0.23.0",
    "@hono/vite-ssg": "^0.3.0",
    "@types/node": "^24.10.3",
    "globby": "^16.0.0",
    "html-minifier-terser": "^7.2.0",
    "prettier": "^3.7.4",
