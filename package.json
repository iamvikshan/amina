{
  "name": "dashboard",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite --config vite.config.mts",
    "start": "bun .",
    "build": "bun run build:honox",
    "build:honox": "bun clean && bun run build:honox:client && bun run build:honox:server",
    "build:honox:client": "vite build --config vite.config.mts --mode client",
    "build:honox:server": "vite build --config vite.config.mts",
    "f": "prettier --write .",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@astrojs/node": "9.5.1",
    "@astrojs/react": "4.4.2",
    "@astrojs/sitemap": "3.6.0",
    "@astrojs/tailwind": "6.0.2",
    "@iconify-json/lucide": "^1.2.80",
    "@preline/accordion": "^3.2.3",
    "@preline/collapse": "^3.2.3",
    "@preline/dropdown": "^3.2.3",
    "@preline/overlay": "^3.2.3",
    "@preline/tabs": "^3.2.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/alpinejs": "^3.13.11",
    "alpinejs": "^3.15.2",
    "astro": "5.16.5",
    "astro-compressor": "^1.2.0",
    "astro-icon": "^1.1.5",
    "gsap": "^3.14.2",
    "hono": "^4.11.0",
    "honox": "^0.1.52",
    "lenis": "^1.3.16",
    "lucide-react": "^0.555.0",
    "mongoose": "^9.0.1",
